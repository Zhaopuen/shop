
.shop_infobox{
  width:94%;
  height:auto;
  /* position: fixed; */
  background: #fafafa;
  overflow: hidden;
  padding:3%;
}
.shop_addInfor{
  width:100%;
  height:auto;
  overflow: hidden;
  position: relative;
}
.shop_addInfo_title{
  color:#444;
  font-size: 32rpx;
}
.shop_addInfo_address{
  width:92%;
  height:100rpx;
  line-height: 100rpx;
  background: #fff;
  border-radius:10rpx;
  margin:30rpx 0;
  padding:0 4%;
}
.shop_addInfo_address text{
  font-size: 30rpx;
  display:block;
  float:left;
}
.shop_addInfo_address image{
  width:60rpx;
  height:60rpx;
  display: block;
  float:right;
  margin-top: 18rpx;
}
.shop_addDetail{
  width:100%;
  height:auto;
  overflow: hidden;
}
.shop_addInfor input{
  display: block;
  width:92%;
  height:100rpx;
  padding:0 4%;
  background: #fff;
  /* box-shadow: 0px 3px 3px 0px 
		rgba(68, 68, 68, 0.1); */
  border-radius:10rpx;
  /* margin:30rpx 0; */
  font-size: 30rpx;
}
.shop_btn{
  width:85%;
  height:100rpx;
  background-color: #fddb00;
	box-shadow: 0px 10px 20px 0px 
		rgba(253, 219, 0, 0.35);
	border-radius: 50px;
  color:#222;
  line-height: 100rpx;
  text-align: center;
  margin:60rpx auto;
}

/* 获取验证码 */
.shop_inputbox{
  width:100%;
  height:100%;
  overflow: hidden;
  background: #fff;
  margin:30rpx 0;
  box-shadow: 0px 3px 3px 0px 
		rgba(68, 68, 68, 0.1);
}
.yanzhengma{
  margin:15rpx 0 30rpx;
  box-shadow: 0px 3px 3px 0px 
		rgba(68, 68, 68, 0.1);
}
.phone_input{
  height:100%;
  float:left;
  display: block;
}
.changeInfo{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 90%;
  margin: 50rpx auto;
}
.changeInfoName{
  position: relative;
  height: 80rpx;
  width: 100%;
  border-radius: 10rpx;
  background: #fff;
  margin-bottom: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.codeBtn{
  /* position: absolute;
  right: 0;
  top: 0; */
  color: #bbb;
  width: 25%;
  font-size: 26rpx;
  height: 100rpx;
  line-height: 100rpx;
  float:right;
  text-align: center;
}
.changeInfoName input{
  width: 100%;
  height:100%;
}
.changeBtn{
  width: 40%;
  height: 100rpx;
  background: #fff;
  color: #000;
  border-radius: 50rpx;
  position: absolute;
  bottom: 10%;
  left: 50%;
  margin-left: -20%;
  line-height: 100rpx;
}
.shop_addImgss{
  width:30rpx;
  height:30rpx;
  right:30rpx;
  top:90rpx;
  position: absolute;
}

.collection_shop{
  width:100%;
  height: auto;
  overflow:hidden;
  background: #FAFAFA;
}

.collection_shoptop{
  width:84%;
  height: auto;
  overflow:hidden;
  background: #fff;
  padding: 3% 8% 50rpx;
}
.collection_shoptop_left{
  width: 70%;
  float:left;
  height: auto;
  overflow: hidden;
  font-size: 48rpx;
  color:#333333;
  margin-top: 29rpx;
}
.collection_shoptop_left view{
  height: auto;
  overflow: hidden;
  margin-top: 25rpx;
}
.collection_shoptop_left text{
  display: block;
  font-size:48rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-bottom: 40rpx;
}
.collection_shoptop_left view image{
  width:25rpx;
  height:25rpx;
  display: block;
  float: left;
  margin-right: 15rpx;
  margin-top: 10rpx;
}
.collection_shoptop_left view span{
  font-size:26rpx;
  font-weight:400;
  line-height:48rpx;
  color:rgba(51,51,51,1);
  opacity:1;
  display: block;
  float:left;
}
.collection_shoplistone{
  margin-top: 60rpx;
  position: relative;
  height: auto;
  overflow: hidden;
}
.collection_shoplistone image{
  text-align: center;
  top:0;
  left:0;
  right:0;
  /* margin: auto auto 29rpx; */
}

.collection_shoptop_right{
  /* width: 25%; */
  /* height: auto; */
  overflow: hidden;
  float: right;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 5rpx solid #FDDB00;
}
.collection_shoptop_right image{
  width: 160rpx;
  height: 160rpx;
}
.collection_shoplist{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.collection_shoplistone{
  width: 33.3%;
  text-align: center;
  float: left;
}
.collection_shoplistone image{
  width: 64rpx;
  height: 64rpx;
  display: block;
}
.collection_shoplistone text{
  font-size:24rpx;
  font-weight:400;
  line-height:48rpx;
  color:rgba(51,51,51,1);
  opacity:1;
}

.collection_shopbottom{
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  margin-top: 27rpx;
}
.collection_baddress{
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 30rpx 0;
  border-bottom: 1px solid rgba(226,226,226,0.2);
}
.collection_baddress{
  width: 84%;
  margin: 0% 8%;
  padding: 40rpx 0;
}
.collection_baddress .address{
  width: 40rpx;
  height: 40rpx;
  display: block;
  float: left;
  margin-right: 24rpx;
  margin-top: 3rpx;
}
.collection_baddress text{
  font-size:30rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  opacity:1;
  display: block;
  float: left;
  margin-top: 5rpx;
}
.collection_status{
  /* width:70%; */
  float: right;
  font-size: 34rpx;
}
.collection_status span{
  display: block;
  float: left;
  margin-right: 15rpx;
  width: 370rpx;
  text-align: right;
  color: #DCDCDC;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  font-size: 30rpx;
  margin-top: 5rpx;
}
.collection_baddress .dayu{
  width: 30rpx;
  height: 35rpx;
  float: right;
  margin-top: 8rpx;
}
